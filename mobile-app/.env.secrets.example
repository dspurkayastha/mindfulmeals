# EAS Secrets Configuration
# These secrets should be configured in EAS using:
# eas secret:create --scope project --name SECRET_NAME --value "secret_value"

# iOS Secrets
IOS_DIST_P12_PASSWORD=your_distribution_certificate_password
IOS_PROVISIONING_PROFILE_PASSWORD=your_provisioning_profile_password

# Android Secrets  
ANDROID_KEYSTORE_PASSWORD=your_keystore_password
ANDROID_KEY_PASSWORD=your_key_password
ANDROID_KEYSTORE_ALIAS=your_keystore_alias

# API Keys for Services
SENTRY_AUTH_TOKEN=your_sentry_auth_token
AMPLITUDE_API_KEY=your_amplitude_api_key
ONE_SIGNAL_APP_ID=your_onesignal_app_id

# CI/CD Secrets (for GitHub Actions)
EXPO_TOKEN=your_expo_access_token

# Backend API Secrets
API_SECRET_KEY=your_api_secret_key
JWT_SECRET=your_jwt_secret