{
  "cli": {
    "version": ">= 9.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "image": "latest",
        "resourceClass": "large"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api-dev.mindfulmeals.com",
        "EXPO_PUBLIC_ENV": "development",
        "NPM_CONFIG_WORKSPACES": "false",
        "EAS_BUILD": "true"
      },
      "node": "18.18.0"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest",
        "resourceClass": "large"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api-staging.mindfulmeals.com",
        "EXPO_PUBLIC_ENV": "preview",
        "NPM_CONFIG_WORKSPACES": "false",
        "EAS_BUILD": "true"
      },
      "node": "18.18.0"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "resourceClass": "large"
      },
      "ios": {
        "buildConfiguration": "Release",
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.mindfulmeals.com",
        "EXPO_PUBLIC_ENV": "production",
        "NPM_CONFIG_WORKSPACES": "false",
        "EAS_BUILD": "true"
      },
      "node": "18.18.0"
    },
    "debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "withoutCredentials": true,
        "image": "latest"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "http://localhost:3000",
        "EXPO_PUBLIC_ENV": "debug",
        "EXPO_PUBLIC_DEBUG": "true",
        "NPM_CONFIG_WORKSPACES": "false",
        "EAS_BUILD": "true"
      },
      "node": "18.18.0"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID"
      }
    }
  }
}
